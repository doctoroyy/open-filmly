{
  "name": "open-filmly",
  "version": "1.0.0",
  "description": "open filmly",
  "main": "dist/electron/main.js",
  "private": true,
  "author": {
    "name": "Open Filmly Team",
    "email": "info@openfilmly.com"
  },
  "engines": {
    "node": ">=16.0.0",
    "pnpm": ">=7.0.0"
  },
  "scripts": {
    "dev": "concurrently \"pnpm run dev:next\" \"pnpm run dev:electron\"",
    "dev:next": "next dev",
    "dev:electron": "tsc -p electron/tsconfig.json && cross-env NODE_ENV=development electron .",
    "build": "next build && tsc -p electron/tsconfig.json",
    "start": "electron .",
    "pack": "electron-builder --dir",
    "dist": "electron-builder",
    "lint": "next lint"
  },
  "build": {
    "appId": "com.example.open-filmly",
    "productName": "open filmly",
    "files": [
      "dist/**/*",
      "node_modules/**/*",
      "package.json"
    ],
    "directories": {
      "output": "release"
    },
    "win": {
      "target": "nsis"
    },
    "mac": {
      "target": "dmg"
    },
    "linux": {
      "target": "AppImage"
    },
    "extraMetadata": {
      "main": "dist/electron/main.js"
    },
    "asar": false
  },
  "dependencies": {
    "@hookform/resolvers": "^3.9.1",
    "@radix-ui/react-accordion": "^1.2.2",
    "@radix-ui/react-alert-dialog": "^1.1.4",
    "@radix-ui/react-aspect-ratio": "^1.1.1",
    "@radix-ui/react-avatar": "^1.1.2",
    "@radix-ui/react-checkbox": "^1.1.3",
    "@radix-ui/react-collapsible": "^1.1.2",
    "@radix-ui/react-context-menu": "^2.2.4",
    "@radix-ui/react-dialog": "^1.1.6",
    "@radix-ui/react-dropdown-menu": "^2.1.4",
    "@radix-ui/react-hover-card": "^1.1.4",
    "@radix-ui/react-label": "^2.1.1",
    "@radix-ui/react-menubar": "^1.1.4",
    "@radix-ui/react-navigation-menu": "^1.2.3",
    "@radix-ui/react-popover": "^1.1.4",
    "@radix-ui/react-progress": "^1.1.1",
    "@radix-ui/react-radio-group": "^1.2.2",
    "@radix-ui/react-scroll-area": "^1.2.2",
    "@radix-ui/react-select": "^2.1.4",
    "@radix-ui/react-separator": "^1.1.1",
    "@radix-ui/react-slider": "^1.2.2",
    "@radix-ui/react-slot": "^1.1.1",
    "@radix-ui/react-switch": "^1.1.2",
    "@radix-ui/react-tabs": "^1.1.3",
    "@radix-ui/react-toast": "^1.2.6",
    "@radix-ui/react-toggle": "^1.1.1",
    "@radix-ui/react-toggle-group": "^1.1.1",
    "@radix-ui/react-tooltip": "^1.1.6",
    "asynckit": "^0.4.0",
    "autoprefixer": "^10.4.16",
    "axios": "^1.6.2",
    "better-sqlite3": "^9.4.3",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "cmdk": "1.0.4",
    "combined-stream": "^1.0.8",
    "cross-spawn": "^7.0.3",
    "date-fns": "4.1.0",
    "delayed-stream": "^1.0.0",
    "electron-is-dev": "^2.0.0",
    "embla-carousel-react": "8.5.1",
    "execa": "^5.1.1",
    "follow-redirects": "^1.15.5",
    "form-data": "^4.0.0",
    "get-stream": "^6.0.1",
    "human-signals": "^2.1.0",
    "input-otp": "1.4.1",
    "is-stream": "^2.0.1",
    "isexe": "^2.0.0",
    "lucide-react": "^0.454.0",
    "merge-stream": "^2.0.0",
    "mime-db": "^1.52.0",
    "mime-types": "^2.1.35",
    "mimic-fn": "^2.1.0",
    "next": "14.0.3",
    "next-themes": "^0.4.4",
    "npm-run-path": "^4.0.1",
    "onetime": "^5.1.2",
    "path-key": "^3.1.1",
    "proxy-from-env": "^1.1.0",
    "react": "^18.2.0",
    "react-day-picker": "8.10.1",
    "react-dom": "^18.2.0",
    "react-hook-form": "^7.54.1",
    "react-resizable-panels": "^2.1.7",
    "recharts": "2.15.0",
    "samba-client": "^7.2.0",
    "shebang-command": "^2.0.0",
    "shebang-regex": "^3.0.0",
    "signal-exit": "^3.0.7",
    "sonner": "^1.7.1",
    "strip-final-newline": "^2.0.0",
    "tailwind-merge": "^2.6.0",
    "tailwindcss-animate": "^1.0.7",
    "vaul": "^0.9.6",
    "which": "^2.0.2",
    "zod": "^3.24.1"
  },
  "devDependencies": {
    "@types/better-sqlite3": "^7.6.12",
    "@types/node": "^20.10.0",
    "@types/react": "^18.2.38",
    "@types/react-dom": "^18.2.17",
    "autoprefixer": "^10.4.31",
    "concurrently": "^8.2.2",
    "cross-env": "^7.0.3",
    "css-loader": "^7.1.2",
    "electron": "^27.1.2",
    "electron-builder": "^24.6.4",
    "postcss": "^8.4.31",
    "postcss-loader": "^8.1.1",
    "style-loader": "^4.0.0",
    "tailwindcss": "^3.3.5",
    "typescript": "^5.3.2"
  },
  "pnpm": {
    "onlyBuiltDependencies": [
      "electron",
      "better-sqlite3"
    ]
  }
}
